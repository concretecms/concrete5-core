<?xml version="1.0"?>
<concrete5-cif version="1.0">

    <attributecategories>
        <category handle="event" allow-sets="1" package="">
            <attributetypes>
                <attributetype handle="text"/>
                <attributetype handle="duration"/>
                <attributetype handle="textarea"/>
                <attributetype handle="boolean"/>
                <attributetype handle="date_time"/>
                <attributetype handle="image_file"/>
                <attributetype handle="number"/>
                <attributetype handle="select"/>
                <attributetype handle="address"/>
                <attributetype handle="telephone"/>
                <attributetype handle="url"/>
                <attributetype handle="email"/>
                <attributetype handle="topics"/>
                <attributetype handle="page_selector"/>
                <attributetype handle="user_selector"/>
                <attributetype handle="site"/>
                <attributetype handle="user_group"/>
            </attributetypes>
        </category>
    </attributecategories>

    <attributetypes>
        <attributetype handle="calendar" name="Calendar" package="">
            <categories>
                <category handle="collection"/>
                <category handle="site"/>
                <category handle="site_type"/>
                <category handle="express"/>
            </categories>
        </attributetype>
        <attributetype handle="calendar_event" name="Calendar Event" package="">
            <categories>
                <category handle="collection"/>
                <category handle="express"/>
            </categories>
        </attributetype>
    </attributetypes>

</concrete5-cif>
